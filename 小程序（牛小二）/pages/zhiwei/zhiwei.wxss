/* pages/zhiwei/zhiwei.wxss */
@import "../../common/sj.wxss";

.z {
  position: fixed;
  width: 100%;
  height: 50px;
  z-index: 999;
  line-height: 50px;
  background: #fff;
}

.x {
  display: inline-block;
  width: 50%;
  text-align: center;
}

.v {
  position: absolute;
  top: 50px;
}

.oo {
  position: fixed;
  width: 48px;
  height: 48px;
  background-color: #f90;
  left: 80%;
  top: 89%;
  border-radius: 24px;
}

.pp {
  position: fixed;
  font-size: 16px;
  width: 40px;
  height: 40px;
  text-align: center;
  border-radius: 20px;
  background-color: #f90;
  left: 81%;
  top: 90%;
}
